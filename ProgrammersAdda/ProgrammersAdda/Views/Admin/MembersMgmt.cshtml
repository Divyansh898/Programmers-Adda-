@model List<ProgrammersAdda.Models.Registration>

@{
    ViewBag.Title = "MembersMgmt";
    Layout = "~/Views/Shared/AdminMaster.cshtml";
}



<div class="row">
    <div class="col-sm-1"></div>
    <div class="col-sm-11">
        <h2 style="text-align:center;font-family:'Imprint MT Shadow';color:#ec1a09;font-weight:bold;">MembersMgmt</h2>
<table class="table table-responsive table table-bordered" border="3px solid black" style="background:linear-gradient(Darkorange,red,brown);color:white;">
    <tr>
        <th>
          Name
        </th>
        <th>
          Gender
        </th>
        <th>
         Email
        </th>
        <th>
           Mobile
        </th>
        <th>
           Qualification
        </th>
        <th>
           Profile
        </th>
        
        <th>
           Address
        </th>
        <th>
          Registration Date
        </th>
      
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mob)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quailification)
        </td>
        <td>
         <img src="../Content/profiles/@Html.DisplayFor(modelItem => item.profile)" 
             height="80px" width="80px" />
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.addr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.regdt)
        </td>
       
    </tr>
}

</table>
      </div>
      
</div>